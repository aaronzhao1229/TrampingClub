{
  "name": "boilerplate-fullstack",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "scripts": {
    "start": "node server",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon --watch server server/index.js",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx .",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.354.0",
    "@aws-sdk/client-s3": "^3.354.0",
    "@aws-sdk/client-ses": "^3.354.0",
    "@aws-sdk/cloudfront-signer": "^3.347.0",
    "@aws-sdk/s3-request-presigner": "^3.354.0",
    "@react-pdf/renderer": "^3.1.12",
    "@reduxjs/toolkit": "^1.9.5",
    "autoprefixer": "^10.4.14",
    "aws-sdk": "^2.1399.0",
    "aws-sdk-js-codemod": "^0.14.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bootstrap": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.17.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.5",
    "pg": "^8.7.1",
    "postcss-loader": "^7.3.3",
    "precss": "^4.0.0",
    "react-bootstrap": "^2.7.4",
    "react-hook-form": "^7.45.0",
    "react-toastify": "^9.1.3",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/react": "^12.1.3",
    "babel-loader": "^8.2.2",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "supertest": "^6.2.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "browserslist": "> 2%, not dead"
}
